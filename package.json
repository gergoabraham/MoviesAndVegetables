{
  "name": "movies-and-vegetables",
  "version": "0.2.0",
  "scripts": {
    "dev:tdd": "mocha --watch test/unit/",
    "dev:firefox": "web-ext -s src run",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:eslint:fix": "eslint --ignore-path .gitignore . --fix",
    "lint:webext": "web-ext -s src lint",
    "test": "echo ### COMMIT SUITE ### && npm-run-all --aggregate-output -p lint:* test:*",
    "test:end-to-end": "mocha test/end-to-end --timeout 120000",
    "test:unit": "mocha --forbid-only test/unit/",
    "test:coverage": "nyc --reporter=lcov --reporter=text-summary --reporter=text --all mocha test/unit/ --timeout 60000",
    "ci:coverage": "npm run test:coverage && codecov",
    "build": "web-ext -s src build -i icons/icon.psd"
  },
  "author": "gergooo <abrahamgergo@gmail.com>",
  "license": "no license",
  "devDependencies": {
    "@types/selenium-webdriver": "^4.0.9",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.7.2",
    "eslint": "^7.9.0",
    "eslint-config-google": "^0.14.0",
    "geckodriver": "^1.20.0",
    "jsdom": "^16.4.0",
    "mocha": "^8.1.3",
    "node-cmd": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "web-ext": "^5.0.0"
  },
  "mocha": {
    "recursive": true,
    "file": "./test/setup.js"
  }
}
