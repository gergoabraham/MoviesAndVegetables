{
  "name": "movies-and-vegetables",
  "version": "0.0.0",
  "scripts": {
    "firefox": "web-ext -s src run",
    "eslint": "eslint --ignore-path .gitignore .",
    "webext-lint": "web-ext -s src lint",
    "tests": "mocha test/**/*.test.js",
    "test": "npm run eslint && npm run webext-lint && npm run tests && npm run coverage",
    "coverage": "nyc --reporter=lcov --reporter=text mocha test/**/*.test.js --timeout 60000",
    "coverageCI": "npm run coverage && codecov",
    "fix-eslint": "eslint --ignore-path .gitignore . --fix"
  },
  "author": "gergooo <abrahamgergo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "eslint": "^6.0.1",
    "eslint-config-google": "^0.13.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "web-ext": "^3.1.0"
  }
}
