{
    "env": {
        "browser": true,
        "webextensions": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 0,
        "strict": ["error", "global"],
        "no-const-assign": "error",
        "no-use-before-define": ["error", {"functions":false}],
        "no-undef": "error",
        "require-jsdoc": "off"
    }
}